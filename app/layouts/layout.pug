doctype html
html.no-js(lang='')
  block vars
  head
    meta(charset='utf-8')
    meta(name='description', content='')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    meta(http-equiv="Cache-Control", content="no-cache, no-store, must-revalidate")
    meta(http-equiv="Pragma", content="no-cache")
    meta(http-equiv="Expires", content="0")
    title IFD Article Resource
    link(rel='apple-touch-icon', href='apple-touch-icon.png')
    link(rel="shortcut icon", href="favicon.ico")

    // Place favicon.ico in the root directory

    // build:css styles/vendor.css
    link(rel="stylesheet", href="../bower_components/quill/quill.core.css")
    link(rel="stylesheet", href="../bower_components/quill/quill.snow.css")
    // bower:css
    // endbower
    // endbuild


    // build:css styles/main.css
    link(rel='stylesheet', href='styles/addon.css')
    link(rel='stylesheet', href='styles/main.css')
    // endbuild

    // build:js scripts/vendor/modernizr.js
    // endbuild
    body(data-title=page_title)
      <!--[if lt IE 10]>
      p.browserupgrade You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.
      <![endif]-->
      //.container-fluid
        .row
          div(style="background-color: #444;").col-xs-12
            button(type="button", style="margin-left: 3px;").btn.btn-sm.btn-dark
            button(type="button", style="margin-left: 3px;").btn.btn-sm.btn-dark Button
            button(type="button", style="margin-left: 3px;").btn.btn-sm.btn-dark Add Resources
      div(style="background-color:#222;display:none;").container-fluid#adminToolBar
        .col-xs-12
          button(type="button").btn.btn-dark#editMode Edit
          button(type="button").btn.btn-dark#deleteMode Delete
      .container
        div(style="padding-top:15px;padding-bottom: 15px;").header.pad-tweak
          div.pull-right
            div(style="padding:10px 15px 10px 10px;cursor:pointer;").dropdown
              a(type="button", data-toggle="dropdown").dropdown-toggle#loginDropDownMenu
                span#userLoginEmail
                span.caret
              ul(role="menu", aria-labelledby="loginDropDownMenu").dropdown-menu
                li(role="presentation")
                  a(href="#", tabindex="-1", role="menitem")#navLogout Logout
            div(style="cursor:pointer;").dropdown
              button(type="button", data-toggle="dropdown").dropdown-toggle.form-control#NavDropDownMenu
                span#curPage &nbsp;&nbsp;
                span.caret
              ul(role="menu", aria-labelledby="loginDropDownMenu").dropdown-menu
                li(role="presentation")
                  a(href="search.html", tabindex="-1", role="menitem")#navSearchArticles Search Articles
                li(role="presentation")
                  a(href="upload.html", tabindex="-1", role="menitem")#navUploadArticles Upload Articles
                li.divide
                li(role="presentation").dropdown.dropdown-submenu
                  a(href="#", tabindex="-1", role="menitem", data-toggle="dropdown").dropdown-toggle#navSettings System Settings
                  ul.dropdown-menu
                    li(role="presentation")
                      a(href="account.html", tabindex="-1", role="menitem")#navAccount Account Info
                    li.divider
                    li(role="presentation")
                      a(href="collections.html", tabindex="-1", role="menitem")#navViewCollections Collections
                    li(role="presentation")
                      a(href="users.html", tabindex="-1", role="menitem")#navUsers Users
                    //li(role="presentation")
                      a(href="files.html", tabindex="-1", role="menitem")#navFiles Files
                    li(role="presentation")
                      a(href="setting.html", tabindex="-1", role="menitem")#navMatrix Matrix/Category
          a(href="search.html")
            img(src="images/logo.png", alt="OraSure Logo", style="width: 220px;").img-responsive
        .row.row-tweak
          div(style="width: 100%;height: 30px;background-color:#17527E;")

        block content

        .footer
          .row.row-tweak
            div(style="width: 100%;height: 30px;background-color:#17527E;margin-top: 5px;")
          p &hearts; from the Creative Services
          //button(type="button").btn.btn-default.center-block#storageReset Reset


